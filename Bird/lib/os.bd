create funct exec(txt){
	pyparse `os.system('''@{txt}''')`;
}[global=&req_pyparse=true];
create funct name(){
	pyparse `var['dat'] = {'type':'string','dt'os.name','headers':{}}`
	return dat;
}[req_pyparse=true];
