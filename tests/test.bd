create funct replace(old,new,cnt='*'){
	create var data "test";
	if (`"@{cnt}" == '*'`){
		pyparse `var['data']['dt'] = var['data']['dt'].replace('''@{old}''','''@{new}''')`;
	};
	else{
		pyparse `var['data']['dt'] = var['data']['dt'].replace('''@{old}''','''@{new}''',int(@{cnt}))`;
	};
	return data;
};
